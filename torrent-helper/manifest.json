{
	"name" : "Torrent helper",
	"version" : "0.0.1",
	"manifest_version" : 2,
	"content_scripts" : [
		{
			"matches" : ["*://*.thepiratebay.se/search/*"],
			"js" : ["utilities.js", "piratebay.js"]
		},
		{
			"matches" : ["*://*.imdb.com/title/*"],
			"js" : ["utilities.js", "imdb.js"]
		}
	],
	"permissions" : [
		"*://*.thepiratebay.se/*"
	]
}